create or replace PROCEDURE DROP_DUPS
 (name_in in varchar2)
 AS
BEGIN
   EXECUTE IMMEDIATE 'DELETE FROM ' || name_in || ' WHERE ROWID IN (SELECT ROWID FROM (SELECT ROWID, ROW_NUMBER() OVER (PARTITION BY SYS_ID || TO_CHAR(SYS_UPDATED_ON, ''YYYY/MM/DD HH24:MI:SS'') ORDER BY SYS_ID || TO_CHAR(SYS_UPDATED_ON, ''YYYY/MM/DD HH24:MI:SS'') DESC) DUP FROM ' ||
name_in || ') WHERE DUP > 1)';
EXCEPTION
   WHEN OTHERS THEN
      IF SQLCODE != -942 THEN
         RAISE;
      END IF;
END;